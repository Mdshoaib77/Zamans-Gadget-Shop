{
  "version": 2,
  "builds": [
    {
      "src": "ZammansGadget/backend/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "ZammansGadget/frontend/dist/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "ZammansGadget/backend/server.js"
    }
  ]
}
